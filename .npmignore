# .npmignore file
# Files and directories to exclude from npm package

# Development files
dev/
src/
*.config.ts
*.config.js
tsconfig.json

# Build tools
vite.config.ts
tailwind.config.js

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Dependencies
node_modules/

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Temporary files
*.tmp
*.temp

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# Test files
**/*.test.*
**/*.spec.*
__tests__/

# Documentation source
docs/
examples/

# Git
.git/
.gitignore

# Lock files (optional - many prefer to include them)
pnpm-lock.yaml
yarn.lock
package-lock.json
